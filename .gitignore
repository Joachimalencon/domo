# ----- Symfony (/api) -----

# Cache and logs (Symfony2)
api/app/cache/*
api/app/logs/*
!api/app/cache/.gitkeep
!api/app/logs/.gitkeep

# Email spool folder
api/app/spool/*

# Cache, session files and logs (Symfony3)
api/var/cache/*
api/var/logs/*
api/var/sessions/*
!api/var/cache/.gitkeep
!api/var/logs/.gitkeep
!api/var/sessions/.gitkeep

# Logs (Symfony4)
api/var/log/*
!api/var/log/.gitkeep

# Parameters
api/app/config/parameters.yml
api/app/config/parameters.ini

# Managed by Composer
api/app/bootstrap.php.cache
api/var/bootstrap.php.cache
api/bin/*
!api/bin/console
!api/bin/symfony_requirements
api/vendor/

# Assets and user uploads
api/web/bundles/
api/web/uploads/

# PHPUnit
api/app/phpunit.xml
api/phpunit.xml

# Build data
api/build/

# Composer PHAR
api/composer.phar

# Backup entities generated with doctrine:generate:entities command
**/api/Entity/*~

# Embedded web-server pid file
api/.web-server-pid

api/docker-compose-template.yml

# ----- Vue 3 (/front) -----

# Dependency directories
front/node_modules/

# Production build output
front/dist/
!front/dist/.gitkeep

# Local environment files
front/.env.local
front/.env.*.local


# ----- IDE -----

.idea/
